package trash;

import java.awt.Color;
import java.awt.Graphics;

import head.Drawable;
import head.Tickable;

public class RR implements Tickable, Drawable{

	private int count = 128, diff = 1;
	
	@Override
	public void draw(Graphics g) {
		g.setColor(new Color(count, count, count));
		g.fillRect(200, 200, 400, 200);
	}

	@Override
	public void tick() {
		if(count > 250)
			diff = -1;
		if(count < 5)
			diff = 1;
		count = count + diff;
		System.out.println(count);
	}

	
	
}
